        "zendframework/zendframework1": "1.12.1",
        "ext-mongo": ">=1.3.5,<2.0",
        "ext-json": "~1.2"
    },
    "require-dev": {
        "pear-phpunit/phpunit": "~3.4.15",
        "squizlabs/php_codesniffer": "~1.4",
        "dominionenterprises/dws-coding-standard": "~1.1",
        "lib-libxml": "*",
        "phpseclib/phpseclib": "dev-master"
    },
    "autoload": {
        "psr-0": {
          "": ["application/controllers", "library", "tests", "tests/library", "tests/application", "tests/application/controllers"],
          "Plugin" : "application"
        }
