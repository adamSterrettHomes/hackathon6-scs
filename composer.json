{
    "name": "hackathon6/scs",
    "description": "TOL Widget Builder",
    "keywords": ["hackathon"],
    "authors": [
        {
            "name": "Chad Gray",
            "email": "chad.gray@dominionenterprises.com",
            "role": "Developer"
        }
    ],
    "license": "proprietary",
    "repositories": [
        {
            "type": "composer",
            "url": "http://dominionenterprises.github.io/dws-composer"
        },
        {
            "type": "pear",
            "url": "http://pear.phpunit.de"
        }
